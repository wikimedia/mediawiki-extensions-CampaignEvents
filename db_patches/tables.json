	{
		"name": "ce_organizers",
		"comment": "Stores the organizers of an event.",
		"columns": [
			{
				"name": "ceo_id",
				"type": "bigint",
				"options": {
					"notnull": true,
					"autoincrement": true,
					"unsigned": true
				}
			},
			{
				"name": "ceo_event_id",
				"comment": "Gets the event id from campaign_events table.",
				"type": "bigint",
				"options": {
					"notnull": true,
					"unsigned": true
				}
			},
			{
				"name": "ceo_user_id",
				"comment": "The Central ID.",
				"type": "integer",
				"options": {
					"notnull": true,
					"unsigned": true
				}
			},
			{
				"name": "ceo_role_id",
				"comment": "ID of the role, as determined by the application.",
				"type": "bigint",
				"options": {
					"notnull": true,
					"unsigned": true
				}
			}
		],
		"indexes": [
			{
				"name": "ceo_event_user_role",
				"columns": [ "ceo_event_id", "ceo_user_id", "ceo_role_id" ],
				"unique": true
			}
		],
		"pk": [ "ceo_id" ]
	}
]
